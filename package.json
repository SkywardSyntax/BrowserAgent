{
  "name": "browser-agent",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run server/index.js",
    "smoke:role": "bun run server/smoke_role_click.ts",
    "smoke:disguised": "bun run server/smoke_disguised_button.ts",
    "smoke:image": "bun run server/smoke_image_button.ts",
    "smoke:ai": "bun run server/smoke_ai.ts",
    "build": "rm -rf dist && mkdir -p dist/public && cp -r public/* dist/public/ && bun build public/app/main.js --outdir dist/public/app --target browser",
    "start": "NODE_ENV=production bun run server/index.js",
    "setup": "bun run setup.js",
    "test": "bun run test.js"
  },
  "dependencies": {
    "@langchain/community": "^0.3.54",
    "@langchain/core": "^0.3.74",
    "@langchain/openai": "^0.6.11",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "langchain": "^0.3.33",
    "next": "^14.2.30",
    "openai": "^4.76.1",
    "playwright": "^1.49.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1",
    "ws": "^8.15.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10"
  }
}
